{
  "name": "@fitx/customer-components",
  "version": "4.1.1",
  "files": [
    "dist",
    "src"
  ],
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles.css": "./dist/index.css"
  },
  "type": "module",
  "scripts": {
    "build": "npm run test:unit -- run --coverage  && npm run build:storybook",
    "build:lib": "rm -rf ./dist && vue-tsc --project tsconfig.build-types.json && vite build",
    "build:storybook": "storybook build && npm run temp-fix-copy-storybook",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "temp-fix-copy-storybook": "cp ./coverage/index.html ./storybook-static/coverage/index.html"
  },
  "devDependencies": {
    "@babel/types": "^7.24.0",
    "@chromatic-com/storybook": "^1.4.0",
    "@playwright/test": "^1.43.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-interactions": "^8.1.1",
    "@storybook/addon-links": "^8.1.1",
    "@storybook/blocks": "^8.1.1",
    "@storybook/test": "^8.1.1",
    "@storybook/vue3": "^8.1.1",
    "@storybook/vue3-vite": "^8.1.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.5.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-playwright": "^1.5.4",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "storybook": "^8.1.1",
    "typescript": "~5.4.0",
    "vite": "^5.2.9",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.4.0",
    "vue": "^3.4.22",
    "vue-tsc": "^2.0.13"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0"
  }
}
